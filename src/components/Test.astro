---
// Function that returns the word "render" after 2 seconds
const getRenderTxt = () => {
  return new Promise((resolve) => {
    setTimeout(() => {
      resolve("render");
    }, 2000);
  });
};

const text = await getRenderTxt();
import { Button } from "./Button";

Astro.response.headers.set('CDN-Cache-Control', 'public, max-age=45, must-revalidate');
---

    <Button text={text}/>
